# Tavily API Keys (comma-separated for multiple keys)
TAVILY_API_KEYS=tvly-dev-key1,tvly-dev-key2,tvly-dev-key3

# Or use a single API key (alternative to TAVILY_API_KEYS)
# TAVILY_API_KEY=tvly-dev-your-single-key

# Supergateway configuration
SUPERGATEWAY_PORT=60002
SUPERGATEWAY_BASE_URL=http://0.0.0.0:60002
SUPERGATEWAY_SSE_PATH=/sse
SUPERGATEWAY_MESSAGE_PATH=/message

# Load balancer settings
MAX_ERRORS_PER_KEY=5
KEY_ROTATION_STRATEGY=round_robin